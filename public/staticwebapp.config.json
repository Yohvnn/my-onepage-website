{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/gallery/*",
      "/favicon.ico",
      "/robots.txt",
      "/sitemap.xml",
      "/*.{css,js}",
      "/*.{png,jpg,jpeg,gif,svg,webp,ico}",
      "/*.{woff,woff2,ttf,otf}"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
